# ZZZ

image zzz:
    "mod_assets/sans/zzz.png"
    nearest True

transform zzz_move(z=1):
    SP
    zoom z
    xoffset -10 * z
    yoffset 10 * z
    parallel:
        easeout_cubic 0.75 zoom z * 2
    parallel:
        ease 0.6 xoffset 10 * z
        ease 0.6 xoffset -10 * z
        repeat
    parallel:
        0.3
        block:
            ease 0.6 yoffset -10 * z
            ease 0.6 yoffset 10 * z
            repeat

