transform sans_t_move(z=1):
    xoffset 0 yoffset 0
    block:
        xoffset 0 yoffset -2 * z
        0.15
        xoffset 2 * z yoffset -4 * z
        0.15
        yoffset -2 * z
        0.15
        yoffset 0
        0.15
        xoffset 0 yoffset -2 * z
        0.15
        xoffset -2 * z yoffset -4 * z
        0.15
        yoffset -2 * z
        0.15
        yoffset 0
        0.15
        repeat # 1.2
transform sans_h_move(z=1):
    xoffset 0 yoffset 0
    0.02
    block:
        xoffset 0 yoffset -2 * z
        0.075
        xoffset 1 * z yoffset -1.15 * z
        0.075
        xoffset 2 * z yoffset -4 * z
        0.15
        yoffset -2 * z
        0.15
        yoffset 0
        0.15
        xoffset 0 yoffset -2 * z
        0.075
        xoffset -1 * z yoffset -1.15 * z
        0.075
        xoffset -2 * z yoffset -4 * z
        0.15
        yoffset -2 * z
        0.15
        yoffset 0
        0.15
        repeat
    
transform sans_t_move2(z=1):
    yoffset 0 xoffset 0
    block:
        0.4
        yoffset -2 * z
        0.65
        yoffset -4 * z
        1.1
        yoffset -2 * z
        0.65
        yoffset 0
        0.8
        repeat # 3.6
transform sans_h_move2(z=1):
    xoffset 0 yoffset 0
    block:
        0.3
        yoffset -1.5 * z
        0.3
        yoffset -3 * z
        0.3
        yoffset -4.5 * z
        0.3
        yoffset -6 * z
        0.75
        yoffset -4.5 * z
        0.3
        yoffset -3 * z
        0.3
        yoffset -1.5 * z
        0.3
        yoffset 0
        0.75
        repeat # 3.6


